{
    "expo": {
        "name": "App Confitec Gov",
        "slug": "prefeitura-confitec",
        "platforms": [
            "ios",
            "android",
            "web"
        ],
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "ios": {
            "bundleIdentifier": "com.confitec.appconfitecgov",
            "buildNumber": "1.0.0",
            "supportsTablet": true
        },
        "android": {
            "package": "com.confitec.appconfitecgov",
            "versionCode": 1,
            "adaptiveIcon": {
                "backgroundColor": "#FFFFFF",
                "foregroundImage": "./assets/icon_foreground.png"
            }
        },
        "androidStatusBar": {
            "backgroundColor": "#00000033",
            "translucent": true,
            "barStyle": "light-content"
        },
        "updates": {
            "fallbackToCacheTimeout": 30000
        },
        "assetBundlePatterns": [
            "**/*"
        ]
    }
}